<%= form_for @review, url: {action: "show"} do |f| %>
  <%= f.label :name, class: 'label' 00>
  <%= f.text_field :name, class: 'input' %>
  <br><br>
  <%= f.label :review, class: 'label' %>
  <%= f.text_field :review, class: 'input' %>
  <br><br>
  <%= f.label :rating, class: 'label' %>
  <%= f.number_field :rating, class: 'input' %>
  <br><br>
  <%= f.submit class: 'button is-warning is-rounded' %>
<% end %>